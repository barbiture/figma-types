{
  "name": "figma-types",
  "version": "1.0.0",
  "description": "Typed client code for Figma Platform",
  "main": "index.js",
  "scripts": {
    "prepare": "npm upgrade quicktype && npm run generate:types",
    "generate:types": "npm run generate:schema && ./quicktype.sh",
    "generate:schema":
      "typescript-json-schema 'types/*.ts' Export -o schema.json --required --titles --topRef",
    "start": "watch \"npm run generate:types\" types"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/quicktype/figma-types.git"
  },
  "author": "David Siegel <david@quicktype.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quicktype/figma-types/issues"
  },
  "homepage": "https://github.com/quicktype/figma-types#readme",
  "dependencies": {
    "quicktype": "9.0.32",
    "typescript-json-schema": "^0.21.0",
    "watch": "^1.0.2"
  }
}
